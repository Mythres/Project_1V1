{
  "functions": {
    "api/auth/login/main.go": {
      "memory": 128
    },
    "api/auth/register/main.go": {
      "memory": 128
    },
    "api/auth/update/main.go": {
      "memory": 128
    },
    "api/auth/forgot/main.go": {
      "memory": 128
    }
  },
  "env": {
    "JWT_PUBLIC": "@1v1-jwt-publickey",
    "JWT_PRIVATE": "@1v1-jwt-privatekey",
    "AWS_GAMELIFT_ACCESS_KEY_ID": "@aws-gamelift-key-id",
    "AWS_GAMELIFT_SECRET_KEY": "@aws-gamelift-secret-key"
  },
  "regions": ["cdg1"],
  "trailingSlash": false,
  "rewrites": [
    { "source": "/api/auth/forgot", "destination": "/api/auth/forgot/main" },
    { "source": "/api/auth/login", "destination": "/api/auth/login/main" },
    { "source": "/api/auth/register", "destination": "/api/auth/register/main" },
    { "source": "/api/auth/update", "destination": "/api/auth/update/main" },
    { "source": "/api/game/match", "destination": "/api/game/match/main" }
  ],
  "redirects": [
    { "source": "/", "destination":  "/dev" },
    { "source": "", "destination": "/dev"}
  ]
}
